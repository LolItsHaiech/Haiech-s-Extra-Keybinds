{
  "schemaVersion": 1,
  "id": "haiechextrakeybinds",
  "version": "${version}",
  "name": "HaiechExtraKeybinds",
  "description": "Adds some keybinds for different minecraft settings for convenience",
  "authors": [
    "realHaiech"
  ],
  "contact": {
    "email": "realhaiech@gmail.com",
    "discord": "@realHaiech"
  },
  "license": "MIT",
  "icon": "assets/haiechextrakeybinds/icon.png",
  "accessWidener": "haiechextrakeybinds.accesswidener",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.visiblebarrierblocks.haiechextrakeybinds.client.HaiechextrakeybindsClient"
    ],
    "main": [
      "com.visiblebarrierblocks.haiechextrakeybinds.Haiechextrakeybinds"
    ]
  },
  "mixins": [
    "haiechextrakeybinds.mixins.json",
    {
      "config": "haiechextrakeybinds.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
